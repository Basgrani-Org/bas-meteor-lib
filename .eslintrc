{
    "extends": "@meteorjs/eslint-config-meteor",
    "parser": "babel-eslint",
    "rules": {
        "no-underscore-dangle": 0,
        "no-prototype-builtins": 0,
        "no-restricted-syntax": 0,
        "no-restricted-globals": 0,
        "no-useless-escape": 0,
        "no-irregular-whitespace": 0,
        "default-case": 0,
        "no-nested-ternary": 0,
        "max-len": 0,
        "import/no-dynamic-require": 0,
        "consistent-return": 0,
        "global-require": 0,
        "no-shadow": 0,
        "no-useless-constructor": 0,
        "prefer-destructuring": 0,
        "no-console": 0,
        "no-param-reassign": 0,
        "camelcase": 0,
        "meteor/no-session": ["off", { } ],
        "arrow-body-style": 0,
        "import/prefer-default-export": 0,
        "arrow-parens": [1, "always"],
        "class-methods-use-this": 0,
        "no-case-declarations": 0,
        "import/no-named-as-default": 0
    },
    "globals": {
        "jQuery": true,
        "_": false,
        "$": false,
        "BasMTR": true,
        "Meteor": true,
        "Accounts": true,
        "HTTP": true,
        "ReactiveVar": true,
        "Template": true,
        "Blaze": true,
        "Tracker": true,
        "Match": true,
        "check": true,
        "extend": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "browser": true
    }
}
